datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}
generator client {
  provider = "prisma-client-js"
}
model User {
  id            String    @id @default(uuid()) @db.Uuid
  email         String    @unique
  username      String    @unique
  password      String
  roles         String
  googleId      String?
  provider      String?
  isActive      Boolean   @default(true)
  createdAt     DateTime  @default(now())
  updatedAt     DateTime  @updatedAt
}